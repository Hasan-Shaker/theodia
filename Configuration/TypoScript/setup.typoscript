plugin.tx_theodia {
	view {
		templateRootPaths {
			0 = EXT:theodia/Resources/Private/Templates/
			1 = {$plugin.tx_theodia.view.templateRootPath}
		}

		partialRootPaths  {
			0 = EXT:theodia/Resources/Private/Partials/
			1 = {$plugin.tx_theodia.view.partialRootPath}
